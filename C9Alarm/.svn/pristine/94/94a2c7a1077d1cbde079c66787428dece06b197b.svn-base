package com.tavx.C9Alarm.Form;

/**
 * @author Administrator
 *
 */
public abstract class AlarmType {
	public static final int TYPE_ANIM = 0;
	public static final int TYPE_MOVIE = 1;
	Form mForm;
	int type;
	public void setForm(Form mForm){
		this.mForm = mForm;
	}
	public void setType(int type){
		this.type = type;
	}
	public int getType(){
		return type;
	}
}
