<?xml version="1.0" encoding="UTF-8"?>
    <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
         xmlns:baiduadsdk="http://schemas.android.com/apk/res/com.tavx.C9Alarm"
        
        android:id="@+id/linearLayoutAlarmList"
        android:layout_width="fill_parent"
                       android:background="@color/sao_white" 

        android:layout_height="fill_parent" android:orientation="vertical">

       <ExpandableListView 
           android:id="@+id/list"
              android:cacheColorHint="#00000000"
              android:divider="@color/gray"
              android:dividerHeight="1dp"
              android:childDivider="@color/gray"
            android:layout_width="fill_parent"
           android:layout_height="fill_parent"
           android:groupIndicator="@null"
         ></ExpandableListView>
<TextView 
android:id="@+id/bottomBack"
android:background="@drawable/ad_ad"
    android:layout_width="wrap_content"
android:layout_height="wrap_content" 
android:layout_alignParentBottom="true"
android:layout_centerHorizontal="true"
android:visibility="gone"  ></TextView>

</RelativeLayout>
